{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "es6"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": false,
    "noEmit": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "**/*.ts", "**/*.tsx"],
  "include": ["app/**/*", "pages/**/*", "components/**/*"]
}
